<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<title>login or register</title>
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
	<link rel="stylesheet" type="text/css" href="/res/lib/bootstrap-3.3.0/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="/res/lib/bootstrap-3.3.0/css/bootstrap-theme.min.css">
	<link rel="stylesheet" type="text/css" href="/src/css/login.css">
</head>
<body>

	<div class="container my-cnt">
		<div class="row">
			<div class="col-md-4 col-md-offset-4 panel panel-default">
				<!-- begin panel body -->
				<div class="panel-body">
					
					<!-- begin tabs nav -->
					<ul class="nav nav-tabs j-tab_list_cnt" role="tablist">
					  <li role="presentation" class="active"><a href="#login" role="tab" data-toggle="tab">登录</a></li>
					  <li role="presentation"><a href="#register" role="tab" data-toggle="tab">注册</a></li>
					</ul>
					<!-- end tabs nav -->

					<!-- begin tab content -->
					<div class="tab-content my-tab_cnt">

					  <div role="tabpanel" class="tab-pane active" id="login">
					  	<!-- begin login form -->
						  <form role="form">
							  <div class="form-group form-group-lg">
							    <input type="email" class="form-control" name="username" placeholder="请输入用户名" maxlength="10">
							  </div>
							  <div class="form-group form-group-lg">
							    <input type="password" class="form-control" name="password" placeholder="请输入密码" maxlength="10">
							  </div>
							  <button type="submit" class="btn btn-primary btn-block btn-lg">登录</button>
							</form>
							<!-- end login form -->
					  </div>

					  <div role="tabpanel" class="tab-pane" id="register">
					  	<!-- begin register form -->
					  	<form role="form">
							  <div class="form-group form-group-lg">
							    <input type="email" class="form-control" name="username" placeholder="请输入用户名" maxlength="10">
							  </div>
							  <div class="form-group form-group-lg">
							    <input type="password" class="form-control" name="password" placeholder="请输入密码" maxlength="10">
							  </div>
							  <div class="form-group form-group-lg">
							    <input type="password" class="form-control" name="confirm_password" placeholder="请再次输入密码" maxlength="10">
							  </div>
							  <button type="submit" class="btn btn-primary btn-block btn-lg">注册</button>
							</form>
							<!-- end register form -->
					  </div>

					</div>
					<!-- end tab content -->
				</div>
				<!-- end panel body -->
			</div>
		</div>
	</div>

	<script type="text/javascript" src="/res/lib/jquery-3.1.1.min.js"></script>
	<script type="text/javascript" src="/res/lib/bootstrap-3.3.0/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="/src/javascript/login.js"></script>
</body>
</html>